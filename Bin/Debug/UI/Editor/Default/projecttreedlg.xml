<?xml version="1.0" encoding="utf-8"?>
<skin>
	<layout>
		<Window needalphachannel="1" id="ProjectTreeDialog" padding="10,10,10,10" layer="1" bkgnd.render.type="color" width="200" height="400" bkgnd.render.color="dlg_bkg" font="default">
			<Panel layout.left="0" layout.top="0" layout.height="24" layout.right="0" layout="stack" layout.direction="lefttoright">
				<RadioButton id="radio_layout" styleclass="property_dlg_toolbar_btn" width="80" height="24" text="layout" />
				<RadioButton id="radio_resource" styleclass="property_dlg_toolbar_btn" width="80" height="24" text="resource" />
			</Panel>
			<Panel layout.left="0" layout.top="26" layout.right="0" layout.bottom="0" layout="card">
				<ProjectTreeView id="treelayout" styleclass="editor_listctrl" border="1,1,1,1" transparent="1" bkgnd.render.border="1,1,1,1" expandicon.render.type="theme.treeview.expandcollpase.icon">
					<SystemVScrollBar id="__vscrollbar" width="18" bkgnd.render.type="theme.vscrollbar.bkg">
						<Button id="__scrollbar_up" bkgnd.render.type="theme.vscrollbar.up" height="18" />
						<Button id="__scrollbar_thumb" bkgnd.render.type="theme.vscrollbar.thumb" />
						<Button id="__scrollbar_down" bkgnd.render.type="theme.vscrollbar.down" height="18" />
					</SystemVScrollBar>
					<SystemHScrollBar id="__hscrollbar" height="18" bkgnd.render.type="theme.hscrollbar.bkg">
						<Button id="__scrollbar_left" bkgnd.render.type="theme.hscrollbar.left" width="18" />
						<Button id="__scrollbar_thumb" bkgnd.render.type="theme.hscrollbar.thumb" />
						<Button id="__scrollbar_right" bkgnd.render.type="theme.hscrollbar.right" width="18" />
					</SystemHScrollBar>
				</ProjectTreeView>
				<ProjectTreeView id="treeresource" styleclass="editor_listctrl" visible="hidden" transparent="1" expandicon.render.type="theme.treeview.expandcollpase.icon">
					<SystemVScrollBar id="__vscrollbar" width="18" bkgnd.render.type="theme.vscrollbar.bkg">
						<Button id="__scrollbar_up" bkgnd.render.type="theme.vscrollbar.up" height="18" />
						<Button id="__scrollbar_thumb" bkgnd.render.type="theme.vscrollbar.thumb" />
						<Button id="__scrollbar_down" bkgnd.render.type="theme.vscrollbar.down" height="18" />
					</SystemVScrollBar>
					<SystemHScrollBar id="__hscrollbar" height="18" bkgnd.render.type="theme.hscrollbar.bkg">
						<Button id="__scrollbar_left" bkgnd.render.type="theme.hscrollbar.left" width="18" />
						<Button id="__scrollbar_thumb" bkgnd.render.type="theme.hscrollbar.thumb" />
						<Button id="__scrollbar_right" bkgnd.render.type="theme.hscrollbar.right" width="18" />
					</SystemHScrollBar>
				</ProjectTreeView>
			</Panel>
		</Window>
	</layout>
</skin>
