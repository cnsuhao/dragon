<?xml version="1.0" encoding="utf-8"?>
<skin>
	<layout>
		<Window needalphachannel="1" id="imageviewdlg" layer="1" bkgnd.render.type="color" width="500" height="500" bkgnd.render.color="dlg_bkg" font="default">
			<Button id="btn_add" styleclass="theme_btn" width="75" height="28" layout.left="10" layout.top="5" text="add" />
			<Button id="btn_del" styleclass="theme_btn" width="75" height="28" layout.left="93" layout.top="5" text="del" />
			<ListView id="imagelistview" styleclass="editor_listctrl" width="300" layout.left="10" layout.top="40" layout.bottom="10">
				<HeaderListCtrl id="__header" ncchild="1" height="22" />
				<SystemHScrollBar id="__hscrollbar" height="18" bkgnd.render.type="theme.hscrollbar.bkg">
					<Button id="__scrollbar_left" bkgnd.render.type="theme.hscrollbar.left" width="18" />
					<Button id="__scrollbar_thumb" bkgnd.render.type="theme.hscrollbar.thumb" />
