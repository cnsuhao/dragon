IDR_UIBUILDER_UNOPEN MENU 
BEGIN
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "新建工程",                        ID_NEW_PROJECT
        MENUITEM "打开工程",                        ID_OPEN_PROJECT
        MENUITEM "退出(&X)",                      IDM_EXIT
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于(&A) ...",                  IDM_ABOUT
    END
END

IDR_UIBUILDER_OPEN MENU 
BEGIN
    POPUP "文件(&F)"
    BEGIN
        POPUP "新建"
        BEGIN
            MENUITEM "皮肤",                          ID_NEW_SKIN
            MENUITEM "xml文件",                       ID_NEW_FILE, GRAYED
            MENUITEM "窗口",                          ID_NEW_WINDOW
            MENUITEM "菜单",                          ID_NEW_MENU, GRAYED
        END
        MENUITEM "保存\t(Ctrl+S)",                IDM_SAVE
        MENUITEM "关闭工程",                        ID_CLOSE_PROJECT
        MENUITEM "退出(&X)",                      IDM_EXIT
    END
    POPUP "编辑(&E)"
    BEGIN
        MENUITEM "取消\t(Ctrl+Z)",                IDM_UNDO
        MENUITEM "重做\t(Ctrl+Y)",                IDM_REDO
    END
    POPUP "视图(&V)"
    BEGIN
        MENUITEM "工具箱(&T)",                     IDM_VIEW_TOOLBOX
        MENUITEM "属性(&P)",                      IDM_VIEW_PROPERTY
    END
    POPUP "工程(&P)"
    BEGIN
        MENUITEM "依赖控件(&D)",                    IDM_UIPROJ_DEPENDSCTRLDDL
    END
    POPUP "窗口(&W)"
    BEGIN
        MENUITEM "最小化",                         ID_MENU_MDI_MINIMIZE
        MENUITEM "还原",                          ID_MENU_MDI_RESTORE
        MENUITEM "关闭",                          ID_MENU_MDI_CLOSE
        MENUITEM "关闭所有窗口",                      IDM_MDI_CLOSEALLWINDOW
        MENUITEM "关闭除当前所有窗口",                   32796
        MENUITEM SEPARATOR
        MENUITEM "层叠",                          ID_WINDOW_CASCADE
        MENUITEM "平铺",                          ID_WINDOW_TILE_HORZ
        MENUITEM "整理",                          ID_WINDOW_ARRANGE
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于(&A) ...",                  IDM_ABOUT
    END
END


