<?xml version="1.0" encoding="utf-8" ?>

<skin>
  <layout>
    <Window id="ProjectTreeDialog" padding="10" bkgnd.render.type="color" bkgnd.render.color="dlg_bkg">
      <TabCtrl id="tabctrl" layout.left="0" layout.top="0" layout.right="0" layout.bottom="0" styleclass="tabctrl_win32"
               xbkgnd.render.type="color" bkgnd.render.border="1" bkgnd.render.border.color="0xff898C95" bkgnd.render.color="0xffffffff">
        <item>
          <head>
            <TabCtrlButton text="layout" styleclass="tabctrlbtn_bottom"/>
          </head>
          <content>
            <ProjectTreeView id="treelayout" istransparent="1" bkgnd.render.type="null" skinitem.textrender.type="theme" skinitem.textrender.font="default_bold"/>
          </content>
        </item>

        <xitem>
          <head>
            <TabCtrlButton text="xml doc" styleclass="tabctrlbtn_bottom"/>
          </head>
        </xitem>

        <item>
          <head>
            <TabCtrlButton text="resource" styleclass="tabctrlbtn_bottom"/>
          </head>
          <content>
            <ProjectTreeView id="treeresource" istransparent="1" bkgnd.render.type="null" skinitem.textrender.type="theme" skinitem.textrender.font="default_bold"/>
          </content>
        </item>

      </TabCtrl>
    </Window>

  </layout>
</skin>